    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@figma/plugin-typings": "1.86.0",
    "@svgr/webpack": "^8.1.0",
